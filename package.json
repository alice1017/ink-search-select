{
    "name": "ink-search-select",
    "version": "1.0.1",
    "description": "Incremental Search and Select Component for ink",
    "author": {
        "name": "Alice1017",
        "url": "https://github.com/alice1017"
    },
    "license": "MIT",
    "main": "dist/index.js",
    "engines": {
        "node": ">=11"
    },
    "files": [
        "dist",
        "media"
    ],
    "scripts": {
        "build": "babel src --out-dir dist",
        "eval": "babel src --out-dir dist && node dist",
        "prepare": "yarn run build"
    },
    "keywords": [
        "ink",
        "ink-component"
    ],
    "dependencies": {
        "ink": "^2.1.1",
        "ink-select-input": "^3.1.1",
        "ink-text-input": "^3.1.1",
        "prop-types": "^15.7.2",
        "react": "^16.8.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "ink-testing-library": "^1.0.1"
    },
    "peerDependencies": {
        "ink": "^2.1.1"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ],
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    }
}
